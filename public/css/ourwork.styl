@import 'nib'

#main	
	margin-left 0px
	

#main 
	margin-top: 100px; 
	padding-top: 38px;
	padding-left: 0px
	padding-bottom: 0px;
	padding-right: 0px;
	width: 990px;
	min-height: 538px;
	min-width: 846px;
	background-color: rgba(0, 0, 0, 0.56);
	border-radius 12px 

#whatwedo 
	margin-top 0px
	h2
		margin-left 50px; 
	> * 
		padding 0px
		margin 0
		margin-left 0px	
	ul.imagelist		
		li
			width 230px
			background-color transparent
			padding-left 50px
			padding-top 40px 
			padding-bottom 40px 
			padding-right 40px
			font-size 12px
			img
				padding 0
			.launchbutton
				margin-top 10px 
				padding 10px 
				text-align center
				background-color rgba(255,255,255,0.1)
				border-radius 6px
				&:hover
					color #00c2f3
	#description
		margin-left 150px
		font-family ArialMT
		color #f3f3f3
		font-size 14px
		width 320px 
		height 208px

#backbone
	width 143px
	height 143px
	float left 	
	img
		display block		
		padding 30px
		background-color rgba(255, 255, 255, 0.1)
		border-radius 12px
		margin-top 60px
		margin-left 100px
		margin-right 60px

#quote
	padding-bottom 70px
	border-bottom dotted 1px #6b6b6b	

#getquote
	#text
		width 230px
		margin-top 20px
		float left
	margin-top: 100px
	width 430px
	padding-left 140px
	padding-top 50px
	padding-bottom 50px	
	font-family Rockwell
	color #00c2f3
	font-size 36px
	background-color rgba(0, 0, 0, 0.18)
	border-radius 14px

#footer
	margin-top 30px 
	padding-top 0px
		


